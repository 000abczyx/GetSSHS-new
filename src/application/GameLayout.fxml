                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="2" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="3" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.rowIndex="1" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.rowIndex="2" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.rowIndex="3" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                        <Pane maxHeight="80.0" maxWidth="80.0" style="-fx-background-color: #C2C0BF;" styleClass="grid-cell" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                     </children>
                  </GridPane>
                  <AnchorPane fx:id="tileContainer" maxHeight="370.0" maxWidth="370.0" minHeight="370.0" minWidth="370.0" />
                  <VBox fx:id="messageContainer" alignment="CENTER" minHeight="370.0" minWidth="370.0" style="-fx-background-color: lightgrey;" visible="false">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 30;" text="Game Over" x="150.0" y="185.0">
                           <VBox.margin>
                              <Insets top="50.0" />
                           </VBox.margin>
                        </Text>
                        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="60.0">
                           <children>
                              <Label fx:id="retryButton" alignment="CENTER" style="-fx-background-color: darkgrey;" text="Retry">
                                 <padding>
