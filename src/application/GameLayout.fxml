<BorderPane prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Actuator">
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="container" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="420.0" styleClass="heading">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
               <children>
                  <Label style="-fx-font-weight: 700; -fx-font-family: &quot;clear sans&quot;; -fx-font-size: 32px; -fx-text-fill: #a31f34;" styleClass="title" text="GetSSHS" textFill="#a31f34">
                     <HBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                     </HBox.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets right="100.0" />
                     </padding>
                  </Label>
                  <Label fx:id="score" maxHeight="-Infinity" minHeight="40.0" prefHeight="60.0" style="-fx-font-family: &quot;clear sans&quot;; -fx-font-weight: bold; -fx-font-size: 15; -fx-text-alignment: center;" text="SCORE">
                     <padding>
                        <Insets left="8.0" right="8.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="bestscore" maxHeight="-Infinity" minHeight="40.0" prefHeight="60.0" style="-fx-font-family: &quot;clear sans&quot;; -fx-font-weight: bold; -fx-font-size: 15; -fx-text-alignment: center;" text="BEST">
                     <padding>
                        <Insets left="8.0" right="8.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <Label contentDisplay="BOTTOM" text="Some Description">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <AnchorPane prefHeight="143.0" prefWidth="420.0">
               <VBox.margin>
                  <Insets top="-180.0" />
               </VBox.margin>
            </AnchorPane>
            <Pane maxHeight="350.0" maxWidth="350.0" minHeight="370.0" minWidth="370.0">
               <children>
                  <GridPane hgap="10.0" maxHeight="370.0" maxWidth="370.0" minHeight="370.0" minWidth="370.0" style="-fx-background-color: #8A8B8C;" styleClass="game-container" vgap="10.0">
